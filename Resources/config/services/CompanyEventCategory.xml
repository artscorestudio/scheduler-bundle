<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

	<parameters>
    	<!-- DVI Event Category Manager -->
		<parameter key="asf_scheduler.company_event_category.manager.class">ASF\SchedulerBundle\Entity\Manager\CompanyEventCategoryManager</parameter>
		<parameter key="asf_scheduler.company_event_category.manager.entity.class">ASF\SchedulerBundle\Entity\CompanyEventCategory</parameter>
    </parameters>

    <services>
        <!-- Entity Manager -->
        <service id="asf_scheduler.company_event_category.manager" class="%asf_scheduler.company_event_category.manager.class%">
            <tag name="asf_core.manager" entity="%asf_scheduler.company_event_category.manager.entity.class%" />
        </service>
    </services>
    
</container>
