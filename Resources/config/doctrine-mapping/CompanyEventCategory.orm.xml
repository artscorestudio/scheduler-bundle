<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
	http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
	
	<entity name="ASF\SchedulerBundle\Entity\CompanyEventCategory" table="asf_scheduler_company_event_category">

		<id name="id" type="integer" column="id">
			<generator strategy="AUTO" />
		</id>
		
		<field name="state" type="string" length="255" nullable="false" />
		
		<field name="title" column="title" type="string" nullable="false" />
		
		<field name="bgColor" column="bgColor" type="string" nullable="true" />
		
		<field name="fgColor" column="fgColor" type="string" nullable="true" />
		
		<field name="cssClassName" column="cssClassName" type="string" nullable="true" />

	</entity>
	
</doctrine-mapping>